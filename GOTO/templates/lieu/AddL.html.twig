    {% extends 'admin.html.twig' %}
    {% block title %} Goto Bookings {% endblock %}

    {% block admin_body %}
    <body class="fixed-nav sticky-footer" id="page-top">
    {% endblock %}
    {% block content %}
        <div class="content-wrapper">
            <div class="container-fluid">
                <!-- Breadcrumbs-->
                <ol class="breadcrumb">
                    <li class="breadcrumb-item">
                        <a href={{ asset('#') }}>Dashboard</a>
                    </li>
                    <li class="breadcrumb-item active">Add Listing</li>
                </ol>
                <div class="box_general">
                    <div class="header_box">
                        <h2 class="d-inline-block">Ajout Lieu(Restaurant/Bar/Caf√©) </h2>
                        <div class="filter">
                            <select name="orderby" class="selectbox">
                                <option value="Any status">Any status</option>
                                <option value="Approved">Approved</option>
                                <option value="Pending">Pending</option>
                                <option value="Cancelled">Cancelled</option>
                            </select>
                        </div>
                    </div>
                    {# Adam: a charger avec la liste des reservations #}
                    <div class="list_general">
                        {# Adam: insert la table  #}

                        <div class="hero_single version_2">
                            <div class="opacity-mask" data-opacity-mask="rgba(0, 0, 0, 0.6)">
                                <div class="bg_gray pattern" id= "submit">
                                    <div class="container margin_60_40">
                                        <div class="row justify-content-center" align="left">
                                            <div class="col-lg-5">
                                                <div id="message-register"></div>
                                                <form method="post" action="AddL" id="register">

                                                    <div class="row">
                                                        <div class="col-lg-12">
                                                            <div class="form-group">
                                                                {{ form_row(f.Nom) }}
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <!-- /row -->
                                                    <div class="row">
                                                        <div class="col-lg-12">
                                                            <div class="form-group">
                                                                {{ form_row(f.Description) }}
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <!-- /row -->
                                                    <div class="row">
                                                        <div class="col-lg-12">
                                                            <div class="form-group">
                                                                {{ form_row(f.MenuFile) }}
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <!-- /row -->
                                                    <div class="row">
                                                        <div class="col-lg-12">
                                                            <div class="form-group">
                                                                {{ form_row(f.Budget) }}
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <!-- /row -->
                                                    <div class="row ">
                                                        <div class="col-lg-12">
                                                            <div class="form-group">
                                                                {{ form_row(f.categorie) }}
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <!-- /row -->
                                                    <div class="row">
                                                        <div class="col-lg-12">
                                                            <div class="form-group">
                                                                {{ form(f)}}

                                                            </div>
                                                        </div>
                                                    </div>
                                                    <!-- /row -->

                                                </form>
                                            </div>
                                        </div>
                                        <!-- /container -->
                                    </div>
                                </div>
                            </div>
                        </div>

                    </div>
                </div>
                <!-- /box_general-->
                <nav aria-label="...">
                    <ul class="pagination pagination-sm add_bottom_30">
                        <li class="page-item disabled">
                            <a class="page-link" href="{{ asset('#') }}" tabindex="-1">Previous</a>
                        </li>
                        <li class="page-item"><a class="page-link" href="{{ asset('#') }}">1</a></li>
                        <li class="page-item"><a class="page-link" href="{{ asset('#') }}">2</a></li>
                        <li class="page-item"><a class="page-link" href="{{ asset('#') }}">3</a></li>
                        <li class="page-item">
                            <a class="page-link" href="{{ asset('#') }}">Next</a>
                        </li>
                    </ul>
                </nav>
                <!-- /pagination-->
            </div>
            <!-- /container-fluid-->
        </div>
        <!-- /container-wrapper-->
    {% endblock %}





